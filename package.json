{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "auth0-lock": "^10.21.1",
    "bootstrap": "4.0.0-beta",
    "bootstrap-datepicker": "^1.7.1",
    "bootstrap-switch": "^3.3.4",
    "node-sass-chokidar": "^0.0.3",
    "nouislider": "^10.1.0",
    "now-ui-kit": "^1.0.1",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules ./node_modules/now-ui-kit/assets/sass/ src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules ./node_modules/now-ui-kit/assets/sass/ src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build"
  }
}
